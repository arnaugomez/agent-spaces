# Agent Spaces Environment Variables
# Copy this file to .env.local and fill in the values

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=debug

# Database
DATABASE_URL=./data/agent-spaces.db

# Docker / Sandbox
DOCKER_HOST=unix:///var/run/docker.sock
SANDBOX_BASE_IMAGE=oven/bun:1
SANDBOX_TIMEOUT=30000
SANDBOX_MAX_FILE_SIZE=10485760

# API Authentication
API_KEY_SECRET=your-secret-for-signing-keys
AUTH_DISABLED=true

# LLM Evals (Optional)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
EVAL_MODEL=gpt-4o-mini
