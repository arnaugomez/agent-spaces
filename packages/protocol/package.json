{
  "name": "@agent-spaces/protocol",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && tsc --emitDeclarationOnly",
    "dev": "bun build ./src/index.ts --outdir ./dist --target bun --watch",
    "test": "bun test",
    "test:unit": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "*"
  },
  "devDependencies": {
    "typescript": "*",
    "bun-types": "*"
  }
}

